{
  "name": "chrome-extension-tarou",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.25.0",
  "scripts": {
    "dev": "vite --port 1234 --open",
    "ba": "run-s clear build:alp build:prepare build:js",
    "bp": "run-s clear build:prod build:prepare build:js",
    "build:alp": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:prepare": "tsx scripts/prepare.ts",
    "build:js": "vite build --config vite.config.content.mts && vite build --config vite.config.inject.mts",
    "zip": "tsx scripts/zip.ts",
    "log": "tsx scripts/log.ts",
    "clear": "rimraf --glob dist",
    "up": "taze major -I",
    "lint": "eslint --cache ."
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.5.1",
    "@iconify/json": "^2.2.416",
    "@iconify/vue": "^5.0.0",
    "@types/ali-oss": "^6.16.13",
    "@types/archiver": "^7.0.0",
    "@types/chrome": "^0.1.32",
    "@types/jquery": "^3.5.33",
    "@types/lodash-es": "^4.17.12",
    "@types/md5": "^2.3.6",
    "@types/node": "^24.10.2",
    "@unocss/eslint-config": "^66.5.10",
    "@unocss/reset": "^66.5.10",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vueuse/components": "^14.1.0",
    "@vueuse/core": "^14.1.0",
    "@vueuse/shared": "^14.1.0",
    "@zumer/snapdom": "^2.0.1",
    "ali-oss": "^6.23.0",
    "archiver": "^7.0.1",
    "cheerio": "^1.1.2",
    "clipboardy": "^5.0.1",
    "copy-text-to-clipboard": "^3.2.2",
    "dayjs": "^1.11.19",
    "echarts": "5.6.0",
    "element-plus": "^2.12.0",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.1.0",
    "fast-glob": "^3.3.3",
    "lodash-es": "^4.17.21",
    "md5": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.1.2",
    "tsx": "^4.21.0",
    "typescript": "5.7.3",
    "unocss": "^66.5.10",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-vue-components": "^30.0.0",
    "uuid": "^13.0.0",
    "vite": "^7.2.7",
    "vue": "^3.5.25",
    "vue-draggable-next": "^2.3.0",
    "webext-bridge": "^6.0.1"
  }
}
