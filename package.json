{
  "name": "chrome-extension-tarou",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.16.1",
  "scripts": {
    "dev": "vite --port 1234 --open",
    "ba": "run-s clear build:alp build:prepare build:js",
    "bp": "run-s clear build:prod build:prepare build:js",
    "build:alp": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:prepare": "tsx scripts/prepare.ts",
    "build:js": "vite build --config vite.config.content.mts && vite build --config vite.config.inject.mts",
    "zip": "tsx scripts/zip.ts",
    "log": "tsx scripts/log.ts",
    "clear": "rimraf --glob dist",
    "up": "taze major -I",
    "lint": "eslint --cache ."
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.3.0",
    "@iconify/json": "^2.2.384",
    "@iconify/vue": "^5.0.0",
    "@types/ali-oss": "^6.16.11",
    "@types/archiver": "^6.0.3",
    "@types/chrome": "^0.1.11",
    "@types/jquery": "^3.5.33",
    "@types/lodash-es": "^4.17.12",
    "@types/md5": "^2.3.5",
    "@types/node": "^24.5.0",
    "@types/uuid": "^10.0.0",
    "@unocss/eslint-config": "^66.5.1",
    "@unocss/reset": "^66.5.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vueuse/components": "^14.0.0",
    "@vueuse/core": "^13.9.0",
    "@vueuse/shared": "^13.9.0",
    "@zumer/snapdom": "^1.9.14",
    "ali-oss": "^6.23.0",
    "archiver": "^7.0.1",
    "cheerio": "^1.1.2",
    "clipboardy": "^4.0.0",
    "copy-text-to-clipboard": "^3.2.1",
    "dayjs": "^1.11.18",
    "echarts": "^5.6.0",
    "element-plus": "^2.11.2",
    "eslint": "^9.35.0",
    "eslint-plugin-format": "^1.0.1",
    "fast-glob": "^3.3.3",
    "lodash-es": "^4.17.21",
    "md5": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.5",
    "typescript": "5.7.3",
    "unocss": "^66.5.1",
    "unplugin-auto-import": "^20.1.0",
    "unplugin-vue-components": "^29.0.0",
    "uuid": "^13.0.0",
    "vite": "^7.1.5",
    "vue": "^3.5.21",
    "vue-draggable-next": "^2.3.0",
    "webext-bridge": "^6.0.1"
  }
}
